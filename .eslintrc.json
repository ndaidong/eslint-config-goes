{
  "parserOptions": {
    "ecmaVersion": 8,
    "parser": "esprima",
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "globals": {

  },
  "plugins": [

  ],
  "extends": ["eslint:recommended", "google"],
  "rules": {
    "arrow-spacing": [
      "error",
      {
        "before": true,
        "after": true
      }
    ],
    "function-paren-newline": 0,
    "indent": [
      "error",
      2,
      {
        "MemberExpression": 1,
        "FunctionDeclaration": {
          "body": 1,
          "parameters": 2
        }
      }
    ],
    "max-len": [
      "error",
      {
        "code": 120,
        "ignoreTrailingComments": true,
        "ignoreComments": true,
        "ignoreUrls": true
      }
    ],
    "no-unused-vars": 1,
    "no-use-before-define": [
      "error",
      {
        "functions": true,
        "classes": true
      }
    ],
    "space-infix-ops": [
      "error",
      {
        "int32Hint": false
      }
    ]
  }
}
